        </div>

        <h4>Notes</h4>
        @foreach (var item in Model.Notes)
        {
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="row rounded p-3 mb-2 table-active text-dark">
                <div class="col-md-3 col-md-offset-1">
