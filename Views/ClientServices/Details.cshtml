                                <div class="col-md-4 col-md-offset-1">
                                    <div class="form-group">
                                        <label asp-for="Client.insurance2" class="control-label"></label>
                                        <select asp-for="Client.insurance2" class="form-control" asp-items="@ViewBag.ZinsuranceId" disabled>
                                            <option value="" selected disabled>Select Insurance</option>
                                        </select>
