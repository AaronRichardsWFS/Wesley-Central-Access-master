                        <label asp-for="Inquiry.ZworkerId" class="control-label"></label>
                        <select asp-for="Inquiry.ZworkerId" class="form-control" asp-items="@ViewBag.ZworkerId" disabled>
                            <option value="" selected disabled>@Html.DisplayFor(model => model.Inquiry.Zworker.worker)</option>
                        </select>
                        <span asp-validation-for="Inquiry.ZworkerId" class="text-danger"></span>
                    </div>
                </div><br style="line-height:5px;" />
