                    <div class="col-md-6 col-md-offset-1">
                        <label asp-for="Inquiry.ZsiteId" class="control-label"></label>
                        <select asp-for="Inquiry.ZsiteId" class="form-control" asp-items="@ViewBag.ZsiteId" disabled>
                            <option value="" selected disabled>@Html.DisplayFor(model => model.Inquiry.Zsite.site)</option>
                        </select>
                        <span asp-validation-for="Inquiry.ZsiteId" class="text-danger"></span>
                    </div>
                    <div class="col-md-6 col-md-offset-1">
                        <label asp-for="Inquiry.site2" class="control-label"></label>
                        <input asp-for="Inquiry.site2" class="form-control" disabled />
                        <span asp-validation-for="Inquiry.site2" class="text-danger"></span>
                    </div><p style="font-size:5px" class="text-white">_</p>
                </div>
                @{var sitev = @Model.Inquiry.Zsite != null ? @Model.Inquiry.Zsite.site : null;}
                @if (sitev == "School")
                {
                    <div class="row">
                        <div class="col-md-12 col-md-offset-1">
                            <label asp-for="Inquiry.ZschoolId" class="control-label"></label>
                            <select asp-for="Inquiry.ZschoolId" class="form-control" asp-items="@ViewBag.ZschoolId" disabled>
                                <option value="" selected disabled>@Html.DisplayFor(model => model.Inquiry.Zschool.displayname)</option>
                            </select>
                            <span asp-validation-for="Inquiry.ZschoolId" class="text-danger"></span>
                        </div><p style="font-size:5px" class="text-white">_</p>
                    </div>
                }
                @if ((servv == "BHRS") || (servv == "IFC") || (servv == "WK"))
                {
                    <div class="row">
                        <div class="col-md-4 col-md-offset-1">
                            <label asp-for="Inquiry.psychologist" class="control-label"></label>
                            <input asp-for="Inquiry.psychologist" class="form-control" disabled />
                            <span asp-validation-for="Inquiry.psychologist" class="text-danger"></span>
                        </div>
                        <div class="col-md-4 col-md-offset-1">
                            <label asp-for="Inquiry.eval_date" class="control-label"></label>
                            <input asp-for="Inquiry.eval_date" class="form-control" disabled />
                            <span asp-validation-for="Inquiry.eval_date" class="text-danger"></span>
                        </div>
                        <div class="col-md-4 col-md-offset-1">
                            <label asp-for="Inquiry.pocdates" class="control-label"></label>
                            <textarea asp-for="Inquiry.pocdates" class="form-control" disabled></textarea>
                            <span asp-validation-for="Inquiry.pocdates" class="text-danger"></span>
                        </div><p style="font-size:5px" class="text-white">_</p>
                    </div>
                    <div class="row">
                        <div class="col-md-4 col-md-offset-1">
                            <label asp-for="Inquiry.diagnosis" class="control-label"></label>
                            <input asp-for="Inquiry.diagnosis" class="form-control" disabled />
