                                        <textarea asp-for="Client.ins_other_note" class="form-control" disabled></textarea>
                                        <span asp-validation-for="Client.ins_other_note" class="text-danger"></span>
                                    </div>
                                </div>
                            </div><hr /><br style="line-height:5px;" />
