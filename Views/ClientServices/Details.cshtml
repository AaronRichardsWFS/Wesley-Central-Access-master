        <h3 class="mt-3">Inquiry Details</h3>
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="row justify-content-between">
            <div class="col-md-8 col-md-offset-1 rounded p-3 mb-2 table-active text-dark">
