                            <input asp-for="Inquiry.schoolloc" class="form-control" disabled />
                            <span asp-validation-for="Inquiry.schoolloc" class="text-danger"></span>
                        </div><p style="font-size:5px" class="text-white">_</p>
                    </div>
                    <div class="row">
                        <div class="col-md-12 col-md-offset-1">
                            <label asp-for="Inquiry.prescription" class="control-label"></label>
                            <textarea asp-for="Inquiry.prescription" class="form-control" disabled></textarea>
                            <span asp-validation-for="Inquiry.prescription" class="text-danger"></span>
                        </div><p style="font-size:5px" class="text-white">_</p>
                    </div>
                    <div class="row">
                        <div class="col-md-12 col-md-offset-1">
                            <label asp-for="Inquiry.family_avail" class="control-label"></label>
                            <textarea asp-for="Inquiry.family_avail" class="form-control" disabled></textarea>
                            <span asp-validation-for="Inquiry.family_avail" class="text-danger"></span>
                        </div>
                    </div><p style="font-size:5px" class="text-white">_</p>
                }
                <hr />
                <div class="row">
                    <div class="col-md-6 col-md-offset-1">
                        <label asp-for="Inquiry.trackdate" class="control-label"></label>
                        <input asp-for="Inquiry.trackdate" class="form-control" disabled></input>
                        <span asp-validation-for="Inquiry.trackdate" class="text-danger"></span>
                    </div>
                    <div class="col-md-6 col-md-offset-1">
                        <label asp-for="Inquiry.tracknote" class="control-label"></label>
                        <input asp-for="Inquiry.tracknote" class="form-control" disabled></input>
                        <span asp-validation-for="Inquiry.tracknote" class="text-danger"></span>
                    </div>
                </div>
            </div>
            <div class="col-md ml-1 col-md-offset-1 rounded p-3 mb-2 table-active text-dark">
                <div class="row">
                    <div class="col-md-12 col-md-offset-1">
                        <label asp-for="Inquiry.ZstatusId" class="control-label"></label>
                        <select asp-for="Inquiry.ZstatusId" class="form-control" asp-items="@ViewBag.ZstatusId" disabled>
                            <option value="" selected disabled>@Html.DisplayFor(model => model.Inquiry.Zstatus.inq_status)</option>
                        </select>
                        <span asp-validation-for="Inquiry.ZstatusId" class="text-danger"></span>
                    </div>
                </div><br style="line-height:5px;" />
                <div class="row">
                    <div class="col-md-12 col-md-offset-1">
                        <label asp-for="Inquiry.ZreasonId" class="control-label"></label>
                        <select asp-for="Inquiry.ZreasonId" class="form-control" asp-items="@ViewBag.ZreasonId" disabled>
                            <option value="" selected disabled>@Html.DisplayFor(model => model.Inquiry.Zreason.final_reason)</option>
                        </select>
                        <span asp-validation-for="Inquiry.ZreasonId" class="text-danger"></span>
                    </div>
                </div><br style="line-height:5px;" />
