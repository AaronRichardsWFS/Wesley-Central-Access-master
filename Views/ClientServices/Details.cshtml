                    <select asp-for="@item.ZworkerId" class="form-control" asp-items="@ViewBag.ZworkerId" disabled></select>
                </div>
                <div class="col-md-3 col-md-offset-1">
                    <label asp-for="@item.ldate" class="control-label"></label>
                    <input asp-for="@item.ldate" class="form-control" disabled />
