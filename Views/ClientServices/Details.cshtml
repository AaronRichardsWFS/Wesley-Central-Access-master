                    <div class="col-md-6 col-md-offset-1">
                        <label asp-for="Inquiry.ClientId" class="control-label"></label>
                        <input type="hidden" asp-for="Inquiry.ClientId">
                        <br /><p class="font-weight-bold" id="client-selected">@(Model.Client.name)</p>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12 col-md-offset-1">
                        <label asp-for="Inquiry.intext" class="control-label"></label>
                        <select asp-for="Inquiry.intext" class="form-control" disabled>
                            <option selected disabled>@Html.DisplayFor(model => model.Inquiry.intext)</option>
                        </select>
                        <span asp-validation-for="Inquiry.intext" class="text-danger"></span>
                    </div>
                </div><br style="line-height:5px;" />
                @{var intextv = @Model.Inquiry.intext;}
                @if (intextv == "Internal")
                {
                    <div class="row">
                        <div class="col-md-4 col-md-offset-1">
                            <label asp-for="Inquiry.ZinternalId" class="control-label"></label>
                            <select asp-for="Inquiry.ZinternalId" class="form-control" asp-items="@ViewBag.ZinternalId" disabled>
                                <option value="" selected disabled>@Html.DisplayFor(model => model.Inquiry.Zinternal.internal_type)</option>
                            </select>
                            <span asp-validation-for="Inquiry.ZinternalId" class="text-danger"></span>
                        </div>
                        <div class="col-md-8 col-md-offset-1">
                            <label asp-for="Inquiry.intnote" class="control-label"></label>
                            <input asp-for="Inquiry.intnote" class="form-control" disabled />
                            <span asp-validation-for="Inquiry.intnote" class="text-danger"></span>
                        </div><p style="font-size:5px" class="text-white">_</p>
                    </div>
                }
                else if (intextv == "External")
                {
                    <div class="row">
                        <div class="col-md-4 col-md-offset-1">
                            <label asp-for="Inquiry.exttype" class="control-label"></label>
                            <input asp-for="Inquiry.exttype" class="form-control" disabled />
                            <span asp-validation-for="Inquiry.exttype" class="text-danger"></span>
                        </div>
                        <div class="col-md-8 col-md-offset-1">
                            <label asp-for="Inquiry.extnote" class="control-label"></label>
                            <input asp-for="Inquiry.extnote" class="form-control" disabled />
                            <span asp-validation-for="Inquiry.extnote" class="text-danger"></span>
                        </div><p style="font-size:5px" class="text-white">_</p>
                    </div>
                }
                <div class="row">
                    <div class="col-md-6 col-md-offset-1">
                        <label asp-for="Inquiry.ServiceId" class="control-label"></label>
                        <select asp-for="Inquiry.ServiceId" class="form-control" asp-items="@ViewBag.ServiceId" disabled>
                            <option value="" selected disabled>@Html.DisplayFor(model => model.Inquiry.Service.service_desc)</option>
                        </select>
                        <span asp-validation-for="Inquiry.ServiceId" class="text-danger"></span>
                    </div>
                    @{var servv = @Model.Inquiry.Service != null ? @Model.Inquiry.Service.service_desc : null;}
                    @if (servv == "OP")
                    {
                        <div class=col-md-6 col-md-offset-1">
                            <label asp-for="Inquiry.optype" class="control-label"></label>
                            <input asp-for="Inquiry.optype" class="form-control" disabled />
                            <span asp-validation-for="Inquiry.optype" class="text-danger"></span>
                        </div>
                    }
                </div><br style="line-height:5px;" />
