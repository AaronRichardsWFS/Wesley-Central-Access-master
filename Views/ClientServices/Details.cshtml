                    <span asp-validation-for="@item.action" class="text-danger"></span>
                </div>
                <div>
                    <a type="button" class="btn wfs-light-blue" asp-controller="Clineitems" asp-action="Edit" asp-route-id="@item.ClineitemId">Edit Note</a>
