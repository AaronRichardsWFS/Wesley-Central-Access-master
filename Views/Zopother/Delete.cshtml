@model WCAProject.Models.Zopother

@{
    ViewData["Title"] = "Delete";
}

<h1>Delete</h1>

<h3>Are you sure you want to delete this?</h3>
<hr />
<div class="row">
    <div class="col-md-4">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="form-group">
            <label asp-for="opother" class="control-label"></label>
            <input asp-for="opother" class="form-control" disabled/>
            <span asp-validation-for="opother" class="text-danger"></span>
        </div>
    </div>
</div>

<form asp-action="Delete">
    <input type="hidden" asp-for="ZopotherId" />
    <input type="submit" value="Delete Other Op" class="btn btn-danger" />
    <a type="button" class="btn btn-secondary" asp-action="Index">Back to All Other Ops</a>
</form>
