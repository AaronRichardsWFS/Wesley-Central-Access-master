@model WCAProject.Models.Zinsurance

@{
    ViewData["Title"] = "Delete";
}

<h1>Delete Insurance: @Html.DisplayFor(model => model.insurance)</h1>
<h3>Are you sure you want to delete this?</h3>
<div class="row">
    <div class="col-md-4">
        <form asp-action="Delete">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="ZinsuranceId" />
            <div class="form-group">
                <label asp-for="insurance" class="control-label"></label>
                <input asp-for="insurance" class="form-control" disabled/>
                <span asp-validation-for="insurance" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="report" class="control-label"></label>
                <input asp-for="report" class="form-control" disabled/>
                <span asp-validation-for="report" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="active" class="control-label"></label>
                <input asp-for="active" class="form-control" disabled/>
                <span asp-validation-for="active" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="hidden" asp-for="ZinsuranceId" />
                <input type="submit" value="Delete Insurance" class="btn btn-danger" />
                <a type="button" class="btn btn-secondary" asp-action="Index">Back to All Insurances</a>
            </div>
        </form>
    </div>
</div>
