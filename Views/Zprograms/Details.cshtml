@model WCAProject.Models.Zprograms

@{
    ViewData["Title"] = "Details";
}

<h1>View Program: @Html.DisplayFor(model => model.programs_id)</h1>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="programs_id" class="control-label"></label>
                <input asp-for="programs_id" class="form-control" disabled/>
                <span asp-validation-for="programs_id" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="program_desc" class="control-label"></label>
                <input asp-for="program_desc" class="form-control" disabled/>
                <span asp-validation-for="program_desc" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="active" class="control-label"></label>
                <input asp-for="active" class="form-control" disabled/>
                <span asp-validation-for="active" class="text-danger"></span>
            </div>
            <div class="form-group">
                <a type="button" class="btn btn-secondary" asp-action="Index">Back to All Programs</a>
            </div>
        </form>
    </div>
</div>
